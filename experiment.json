                    "ex_mandatory": false
                },
                {
                    "names": ["--ca"],
                    "input": false,
                    "ex_has_default": false,
                    "ex_mandatory": false
                },
                {
                    "names": ["--dataset"],
                    "input": true,
                    "ex_has_default": true,
                    "default": 0,
                    "type": "int",
                    "ex_mandatory": false
                },
                {
                    "names": ["--rl"],
                    "input": false,
                    "ex_has_default": false,
                    "ex_mandatory": false
                },
                {
                    "names": ["--baseline"],
                    "input": false,
                    "ex_has_default": false,
                    "ex_mandatory": false
                },
                {
                    "names": ["--train_emb"],
                    "input": false,
                    "ex_has_default": false,
                    "ex_mandatory": false
                }
            ],
            "ins": [
                {
                    "type": "dir",
                    "location": "glove"
                },
                {
                    "type": "dir",
                    "location": "data"
                }
            ],
            "outs": [
                {
                    "type": "dir",
                    "location": "saved_model"
                }
        
            ]
        },
        {
            "cmd": "python test.py",
            "main": "test.py",
            "steps": ["testing"],
            "ex_args_lib": "argparse",
            "args": [
                {
                    "names": ["--toy"],
                    "input": false,
                    "ex_has_default": false,
                    "ex_mandatory": false
                },
                {
                    "names": ["--ca"],
                    "input": false,
                    "ex_has_default": false,
                    "ex_mandatory": false
                },
                {
                    "names": ["--dataset"],
                    "input": true,
                    "ex_has_default": true,
                    "default": 0,
                    "type": "int",
                    "ex_mandatory": false
                },
                {
                    "names": ["--rl"],
                    "input": false,
                    "ex_has_default": false,
                    "ex_mandatory": false
                },
                {
                    "names": ["--baseline"],
                    "input": false,
                    "ex_has_default": false,
                    "ex_mandatory": false
                },
                {
                    "names": ["--train_emb"],
