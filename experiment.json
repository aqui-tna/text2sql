            "ins": [
                {
                    "type": "dir",
                    "location": "glove"
                },
                {
                    "type": "dir",
                    "location": "data"
                }
            ],
            "outs": [
                {
                    "type": "folder",
                    "location": "saved_model"
                }
        
            ]
        }
        
    ]
