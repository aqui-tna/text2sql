{
    "name": "Baidi96/text2sql",
    "url": "https://github.com/aqui-tna/text2sql",
    "python_version": "3",
    "requirements": {
        "req_file": "requirements.txt",
        "pip": ["torch"],
        
        "others": ["tar"]
    },
    "entries": [
        {
            "cmd": "tar -xjvf data.tar.bz2",
            "main": "unknown",
            "steps": ["data-preparation"],
            "args": [],
            "ins": [
                {
                    "type": "file",
                    "location": "data.tar.bz2",
                    "artifact": "dataset",
                    "ex_in_repo": true
                }
            ],
            "outs": [
                {
                    "type": "dir",
                    "location": "data",
                    "artifact": "dataset"
                }
            ]
        },
        {
            "cmd": "bash download_glove.sh",
            "main": "download_glove.sh",
            "steps": ["data-preparation"],
            "args": [],
            "ins": [],
            "outs": [
                {
                    "type": "dir",
                    "location": "glove",
                    "artifact": "dataset"
                }
            ]
        },
        {
            "cmd": "python extract_vocab.py",
            "main": "extract_vocab.py",
            "steps": ["data-preprocessing"],
            "args": [],
            "ins": [
                {
                    "type": "dir",
                    "location": "data",
                    "artifact": "dataset",
                    "ex_in_repo": true
                }
            ],
            "outs": [
                {
                    "type": "file",
                    "location": "glove/word2idx.json",
                    "artifact": "dataset"
                },
                {
                    "type": "file",
                    "location": "glove/usedwordemb.npy",
                    "artifact": "dataset"
                }
        
            ]
        },
        {
            "cmd": "python train.py",
            "main": "train.py",
            "steps": ["training", "main"],
            "args_lib": "argparse",
            "args": [
                {
                    "names": ["--toy"],
                    "input": null,
                    "default": false,
                    "ex_has_default": true,
                    "mandatory": false
                },
                {
                    "names": ["--suffix"],
                    "input": "SUFFIX",
                    "ex_has_default": true,
                    "default": "",
                    "type": "str",
                    "mandatory": false
                },
                {
                    "names": ["--ca"],
                    "input": false,
                    "ex_has_default": false,
                    "mandatory": false
                },
                {
                    "names": ["--dataset"],
                    "input": "DATASET",
                    "ex_has_default": true,
                    "default": 0,
                    "type": "float",
                    "mandatory": false
                },
                {
                    "names": ["--rl"],
                    "input": null,
                    "ex_has_default": false,
                    "mandatory": false
                },
                {
                    "names": ["--baseline"],
                    "input": null,
                    "ex_has_default": false,
                    "mandatory": false
                },
                {
                    "names": ["--train_emb"],
                    "input": false,
                    "ex_has_default": false,
                    "mandatory": false
                }
            ],
            "ins": [
                {
                    "type": "dir",
                    "location": "glove",
                    "artifact": "dataset",
                    "ex_in_repo": true
                },
                {
                    "type": "dir",
                    "location": "data",
                    "artifact": "dataset",
                    "ex_in_repo": true
                }
            ],
            "outs": [
                {
                    "type": "dir",
                    "location": "saved_model"
                }
        
            ]
        },
        {
            "cmd": "python test.py",
            "main": "test.py",
            "steps": ["testing"],
            "ex_args_lib": "argparse",
            "args": [
                {
                    "names": [
                        "--toy"
                    ],
                    "input": false,
                    "dest": "toy",
                    "ex_has_default": true,
                    "default": false,
                    "type": "float",
                    "mandatory": false
                },
                {
                    "names": [
                        "--ca"
                    ],
                    "input": false,
                    "dest": "ca",
                    "ex_has_default": true,
                    "default": false,
                    "type": "float",
                    "mandatory": false
                },
                {
                    "names": [
                        "--dataset"
                    ],
                    "input": true,
                    "dest": "dataset",
                    "ex_has_default": true,
                    "default": 0,
                    "type": "float",
                    "mandatory": true
                },
                {
                    "names": [
                        "--rl"
                    ],
                    "input": false,
                    "dest": "rl",
                    "ex_has_default": true,
                    "default": false,
                    "type": "float",
                    "mandatory": false
                },
                {
                    "names": [
                        "--baseline"
                    ],
                    "input": false,
                    "dest": "baseline",
                    "ex_has_default": true,
                    "default": false,
                    "type": "float",
                    "mandatory": false
                },
                {
                    "names": [
                        "--train_emb"
                    ],
                    "input": false,
                    "dest": "train_emb",
                    "ex_has_default": true,
                    "default": false,
                    "type": "float",
                    "mandatory": false
                }
            ],
            "ins": [
                {
                    "type": "dir",
                    "location": "glove",
                    "artifact": "dataset",
                    "ex_in_repo": true
                },
                {
                    "type": "dir",
                    "location": "data",
                    "artifact": "dataset",
                    "ex_in_repo": true
                }
            ],
            "outs": [
                {
                    "type": "folder",
                    "location": "saved_model"
                }
        
            ]
        }
        
    ],
    "ex_metrics": {
        "metrics_files": [],
        "prints": ["train acc_qm", "dev acc_qm", "best val acc"],
        "plots": [],
        "others": []
    },
    "ex_hyperparams": {
        "from_cmd": [],
        "config_file": [],
        "in_code": ["epoch"]
    }
}